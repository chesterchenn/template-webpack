{
  "name": "template-webpack",
  "sideEffects": false,
  "version": "1.0.1",
  "description": "quick react webpack template",
  "private": true,
  "scripts": {
    "start": "ts-node bin/start.ts",
    "clear": "ts-node bin/clear.ts",
    "init": "ts-node bin/init.ts",
    "init:ts": "ts-node bin/init.ts --type typescript",
    "init:js": "ts-node bin/init.ts --type javascript",
    "build": "webpack --config webpack/webpack.prod.ts"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "cross-spawn": "^7.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@types/cross-spawn": "^6.0.6",
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/webpack-env": "^1.18.5",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "html-webpack-plugin": "^5.6.3",
    "sass": "^1.81.0",
    "sass-loader": "^16.0.3",
    "style-loader": "^4.0.0",
    "thread-loader": "^4.0.4",
    "ts-loader": "^9.5.1",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^6.0.1"
  },
  "keywords": [
    "template",
    "react",
    "webpack"
  ],
  "author": "chesterchenn",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
